}

@article{keilRethinkingUrbanPolitical2016,
	title = {Rethinking urban political ecology from the outside in: greenbelts and boundaries in the post-suburban city},
	volume = {21},
	issn = {1354-9839, 1469-6711},
	shorttitle = {Rethinking urban political ecology from the outside in},
	url = {https://www.tandfonline.com/doi/full/10.1080/13549839.2016.1145642},
	doi = {10.1080/13549839.2016.1145642},
	abstract = {This paper engages the existing literature on Urban Political Ecology (UPE) from the perspective of regulating urban expansion through greenbelts. The paper makes a contribution to a better understanding of suburbanisation and postsuburbanisation which have so far not been at the centre of the concerns of UPE. In an era of global suburbanisation greenbelts differ from similar boundary setting exercises in the past and are as varied as the suburbanisation processes and their governance themselves. While conscious of those varieties, we focus here on the Greater Golden Horseshoe (GGH) greenbelt in Ontario that was created by provincial legislation in 2005. With the 2005 legislation, the Ontario government declared 720,000 hectares off limits for conventional urban development. The Greenbelt Act created an expansive area under protection from the Niagara Peninsula in the south to the Bruce Peninsula in the north, the Niagara Escarpment in the west to a series of moraines in the east. We will argue that the GGH greenbelt has become a prime negotiation space for the overall re-regulation of urban political ecologies in Southern Ontario. Largely surrounding the booming Toronto region, the GGH greenbelt is expansion space and projection screen of a suburbanizing region in search of redeﬁnition.},
	language = {en},
	number = {12},
	urldate = {2022-04-26},
	journal = {Local Environment},
	author = {Keil, Roger and Macdonald, Sara},
	month = dec,
	year = {2016},
	pages = {1516--1533},
	file = {Keil and Macdonald - 2016 - Rethinking urban political ecology from the outsid.pdf:/Users/billy/Zotero/storage/TJJ448GJ/Keil and Macdonald - 2016 - Rethinking urban political ecology from the outsid.pdf:application/pdf;Making space making race reconstituting white privilege in Buckhead Atlanta.pdf:/Users/billy/Zotero/storage/XJZBS8S3/Making space making race reconstituting white privilege in Buckhead Atlanta.pdf:application/pdf},
