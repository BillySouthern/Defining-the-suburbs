	pages = {3458--3467},
	file = {Samsudin and Bradley - 2010 - Nearest neighbour group-based classification.pdf:/Users/billy/Zotero/storage/8JARG9YI/Samsudin and Bradley - 2010 - Nearest neighbour group-based classification.pdf:application/pdf},
}

